datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ChatMessage {
  id          Int      @id @default(autoincrement())
  fromUserId  String
  toUserId    String
  username    String
  avatarUrl   String?
  text        String
  timestamp   DateTime @default(now())
}
