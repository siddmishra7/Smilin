datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model ChatMessage {
  id          Int      @id @default(autoincrement())
  fromUserId  String
  toUserId    String
  username    String
  avatarUrl   String?
  text        String
  timestamp   DateTime @default(now())
}
